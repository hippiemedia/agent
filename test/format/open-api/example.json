                  "hub.callback": {
                    "type": "string"
                  },
                  "hub.secret": {
                    "type": "string"
                  }
                }
              }
            },
            "application/json": {
              "schema": {
                "$ref": "https://localhost/docs/3-api-spec/request/subscribe.json"
              }
            }
          }
        },
        "responses": {
          "default": {
            "description": "ok"
          }
        }
      }
